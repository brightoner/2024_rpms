<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.go.rastech.ptl.cmmn.exchange.dao.ExchangeDao">


 <!-- 메인 팝업 목록 조회 -->
  <select id="selectFinancialExchange" resultType="kr.go.rastech.commons.utils.LowerKeyMap">
	
	 
		SELECT  DEAL_BAS_R
		  FROM  PTL_EXCHANGE
		 WHERE  CREATE_DTTM  = (SELECT MAX(A.CREATE_DTTM)  FROM PTL_EXCHANGE A WHERE RESULT = '1' )
		 limit 1 

  </select>
  
  
  
</mapper>
